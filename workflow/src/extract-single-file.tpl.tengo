self := import("@platforma-sdk/workflow-tengo:tpl")
json := import("json")

self.defineOutputs("file")

self.body(func(inputs) {
	pColumnData := inputs.pColumnData
	inputMap := pColumnData.inputs()
	
	// Extract single file from fully aggregated PColumn (keyLength = 0)
	singleFile := inputMap[json.encode([])]
	
	return {
		file: singleFile
	}
})

